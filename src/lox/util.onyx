use core.list

_to_list :: (values: []$T) -> list.List(T) {
    l := list.make(T);
    for value: values {
        l->push_end(value);
    }
    return l;
}

is_empty :: (o: ?$T) -> bool {
    switch o {
        case .None {
            return true;
        }
        case #default {
            return false;
        }
    }
}
